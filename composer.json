{
    "name": "elao/theme-twitter-bootstrap-3-bundle",
    "type": "symfony-bundle",
    "description": "Twitter Bootstrap 3 theme bundle for ElaoThemeBundle",
    "keywords": ["elao", "theme-bundle"],
    "homepage": "https://github.com/Elao/ElaoThemeTwitterBootstrap3Bundle",
    "licence": "MIT",
    "authors": [
        {
            "name": "Elao",
            "homepage": "http://www.elao.com"
        },
        {
            "name": "Thomas Jarrand",
            "homepage": "https://github.com/Tom32i"
        }
    ],
    "require": {
        "php":               ">=5.3.4",
        "elao/theme-bundle": "1.0.*@dev"
    },
    "autoload": {
        "psr-0": {
            "Elao\\ThemeBundle\\TwitterBootstrap3Bundle": "."
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "target-dir": "Elao/ThemeBundle/TwitterBootstrap3Bundle"
}
