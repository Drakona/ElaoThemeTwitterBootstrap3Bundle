{
    "name": "elao/theme-twitter-bootstrap-3-bundle",
    "type": "symfony-bundle",
    "description": "Twitter Bootstrap 3 theme bundle for ElaoThemeBundle",
    "keywords": ["elao", "theme-bundle", "twitter", "bootstrap"],
    "homepage": "https://github.com/Elao/ElaoThemeTwitterBootstrap3Bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Elao",
            "homepage": "http://www.elao.com"
        },
        {
            "name": "Thomas Jarrand",
            "homepage": "https://github.com/Tom32i"
        }
    ],
    "require": {
        "php":               ">=5.3.4"
    },
    "suggest": {
        "elao/form-bundle":  "~0.1@dev",
        "elao/theme-bundle": "~0.1@dev"
    },
    "autoload": {
        "psr-4": {
            "Elao\\Bundle\\Theme\\TwitterBootstrap3Bundle": "."
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
